<div class="row">
    <div class="col-lg-6 col-md-6 col-sm-6 col-12">
        <div class="card card-warning animate__animated animate__fadeIn animate__slow shadow-lg">
            <div class="card-header d-flex justify-content-between align-items-center">
                <button type="button" class="btn btn-icon icon-left btn-info" data-toggle="modal" data-target="#modalAddUser" data-access="1"><i class="fas fa-plus"></i> Tambah</button>
                <span class="float-right">Pengguna Mahasiswa</span>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-stripped display nowrap table-hover table-bordered table-sm dataTabel">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Nama</th>
                                <th>Nomor HP.</th>
                                <th>Aksi</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% var i=1; %>
                                <% studentUser.forEach(data=> { %>
                                    <tr>
                                        <td>
                                            <%= i++ %>
                                        </td>
                                        <td>
                                            <%= data.name %>
                                        </td>
                                        <td>
                                            <%= data.phone_number %>
                                        </td>
                                        <td class="text-center">
                                            <a href="/patient/<%= data.id %>" class="btn btn-info btn-sm"><i
                                                    class="fa fa-edit"></i></a>
                                            <button type="button" class="btn btn-danger btn-sm bthapus"
                                                data-kode="<%= data.id %>" data-nama="<%= data.name %>"
                                                data-route="/patient/"><i class="fa fa-trash"></i></button>
                                        </td>
                                    </tr>
                                    <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
    <div class="col-lg-6 col-md-6 col-sm-6 col-12">
        <div class="card card-warning animate__animated animate__fadeIn animate__slow shadow-lg">
            <div class="card-header d-flex justify-content-between align-items-center">
                <button type="button" class="btn btn-icon icon-left btn-info" data-toggle="modal" data-target="#modalAddUser" data-access="1"><i
                        class="fas fa-plus"></i> Tambah</button>
                <span class="float-right">Pengguna Pasien</span>
            </div>
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-stripped display nowrap table-hover table-bordered table-sm dataTabel">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Nama</th>
                                <th>Nomor HP.</th>
                                <th>Aksi</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% var i=1; %>
                                <% patientUser.forEach(data=> { %>
                                    <tr>
                                        <td>
                                            <%= i++ %>
                                        </td>
                                        <td>
                                            <%= data.name %>
                                        </td>
                                        <td>
                                            <%= data.phone_number %>
                                        </td>
                                        <td class="text-center">
                                            <a href="/patient/<%= data.id %>" class="btn btn-info btn-sm"><i
                                                    class="fa fa-edit"></i></a>
                                            <button type="button" class="btn btn-danger btn-sm bthapus"
                                                data-kode="<%= data.id %>" data-nama="<%= data.name %>"
                                                data-route="/patient/"><i class="fa fa-trash"></i></button>
                                        </td>
                                    </tr>
                                    <% }) %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>